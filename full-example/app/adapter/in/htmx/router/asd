es posible de alguna manera pasar el objeto route al contexto del get :

package router

import (
	"my-project-name/app/adapter/in/htmx"
	"my-project-name/app/infrastructure/server"

	ioc "github.com/Ignaciojeria/einar-ioc"
	"github.com/labstack/echo/v4"
)

var _ = ioc.Registry(
	newAppRouter,
	server.NewServer,
	htmx.NewIndex)

type appRouter struct {
	server server.Server
	index  htmx.Index
}

func newAppRouter(
	s server.Server,
	index htmx.Index) appRouter {
	
	/*
	routes := []Route{
		{
			Path:   "/",
			Render: index.Render,
		},
	}*/
	/*
	for _, route := range routes {
		s.Router().GET(v.Path, route.Render)
	}
	*/

	return appRouter{}
}

type Route struct {
	Path     string
	Render   func(c echo.Context) error
	Children []Child
}

type Child struct {
	Path   string
	Render func(c echo.Context) error
}
